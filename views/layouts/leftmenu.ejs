<div class="list-group">



    <% if (categories.length >= 0) { %>
        <% categories.forEach((category) => {    %>

            <a href="/categories/<%= category._id %>"

                        class="list-group-item">


                <%= category.name%>
            </a>


        <% }) %>


    <% } else  { %>
        <h1>Kategori Yok</h1>
  <% }%>

</div>